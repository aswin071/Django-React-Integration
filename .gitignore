# Ignore Python compiled files
*.pyc
__pycache__/

# Ignore Django database file
/backend/db.sqlite3

# Ignore macOS-specific .DS_Store files
.DS_Store

# Ignore the virtual environment
/myenv/
/{{project_name}}-venv/
/venv/

# Ignore local Django settings file
**/settings/local.py

# Ignore static and media files
/staticfiles/*
/mediafiles/*

# Ignore VSCode editor-specific files
/.vscode/

# Ignore code coverage result
.coverage
/coverage/

# Ignore PyCharm editor-specific files
.idea/

